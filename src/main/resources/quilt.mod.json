{
	"schema_version": 1,
	"quilt_loader": {
		"group": "io.github.gaming32",
		"id": "qkdeathswap",
		"version": "${version}",
		"metadata": {
			"name": "Quilt Deathswap",
			"description": "Deathswap mod for Quilt",
			"contributors": {
				"Gaming32": "Owner",
				"wagyourtail": "Collaborator"
			},
			"contact": {
				"homepage": "https://github.com/Gaming32/quilt-kotlin-deathswap",
				"issues": "https://github.com/Gaming32/quilt-kotlin-deathswap/issues",
				"sources": "https://github.com/Gaming32/quilt-kotlin-deathswap"
			},
			"license": "MIT",
			"icon": "assets/qkdeathswap/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
